{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/capm", "dest": "index.js" },
    { "src": "/portfolio", "dest": "index.js" },
    { "src": "/trends", "dest": "index.js" },
    { "src": "/projections", "dest": "index.js" },
    { "src": "/monte", "dest": "index.js" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}
