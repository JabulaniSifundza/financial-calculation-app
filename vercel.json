{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/capm", "dest": "index.js" },
    { "src": "/api/portfolio", "dest": "index.js" },
    { "src": "/api/trends", "dest": "index.js" },
    { "src": "/api/projections", "dest": "index.js" },
    { "src": "/api/monte", "dest": "index.js" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}
